
<div id="portfolio_tem" style="margin-top:160px;">
        <div class="container">
            <div class="row">
                <div class="col-md-12 section_header">
                    <h3>Profile</h3>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-2 left_box">
                    <div class="big">
                        <%= image_tag JSON(@geekette.photo_urls)['large'], {style: 'border-radius: 5px;'} %>
                    </div>
                   
                </div>

                <div class="col-sm-10 right_box">
                    <h2><%= @geekette.last_name %></h2>
                    <p><%= @geekette.interests %>
                    </p>
                      
                    <p><%= @geekette.buisness_city %></p>
           
           
           
                </div>
            </div>
        </div>
    </div>